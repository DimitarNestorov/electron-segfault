  "build": {
    "files": [
      "index.html",
      "main.js",
      "renderer.js"
    ],
    "snap": {
      "useTemplateApp": true
    },
    "linux": {
      "target": "snap"
    }
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "20.22.0"
  },
  "dependencies": {
    "@sentry/electron": "0.7.0"
  }
}
